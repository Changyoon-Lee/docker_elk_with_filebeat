{
    "index_patterns": [
        "recon-bars_botnet_controllers*"
    ],
    "template": {
        "settings": {
            "number_of_shards": 1
        },
        "mappings": {
            "_source": {
                "enabled": true
            },
            "properties": {
                "active": {
                    "type": "keyword"
                },
                "cc": {
                    "type": "keyword"
                },
                "controller_uri": {
                    "type": "keyword"
                },
                "family": {
                    "type": "keyword"
                },
                "first_seen": {
                    "type": "date"
                },
                "ip_addr": {
                    "type": "ip"
                },
                "last_seen": {
                    "type": "keyword"
                },
                "port": {
                    "type": "keyword"
                },
                "proto": {
                    "type": "keyword"
                },
                "subfamily": {
                    "type": "keyword"
                },
                "type": {
                    "type": "keyword"
                }
            }
        }
    },
    "priority": 500,
    "_meta": {
        "description": "my custom"
    }
}