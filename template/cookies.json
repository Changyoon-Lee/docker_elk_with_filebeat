{
    "index_patterns": [
        "recon-cookies*"
    ],
    "template": {
        "settings": {
            "number_of_shards": 1
        },
        "mappings": {
            "_source": {
                "enabled": true
            },
            "properties": {
                "cookie": {
                    "type": "keyword"
                },
                "cookie_sha1": {
                    "type": "keyword"
                },
                "dst_cc": {
                    "type": "keyword"
                },
                "dst_ip_addr": {
                    "type": "ip"
                },
                "dst_port": {
                    "type": "keyword"
                },
                "host": {
                    "type": "keyword"
                },
                "language": {
                    "type": "keyword"
                },
                "referer": {
                    "type": "keyword"
                },
                "src_cc": {
                    "type": "keyword"
                },
                "src_ip_addr": {
                    "type": "ip"
                },
                "src_port": {
                    "type": "keyword"
                },
                "timestamp": {
                    "type": "date"
                },
                "uri": {
                    "type": "keyword"
                },
                "user_agent": {
                    "type": "keyword"
                }
            }
        }
    },
    "priority": 500,
    "_meta": {
        "description": "my custom"
    }
}