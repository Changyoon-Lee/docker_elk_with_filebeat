{
    "index_patterns": ["recon-apt_threats_ips*"],
    "template": {
        "settings": {
            "number_of_shards": 1
        },
        "mappings": {
            "_source": {
                "enabled": true
            },
            "properties": {
                "cc": {
                    "type": "keyword"
                },
                "ip_addr": {
                    "type": "ip"
                },
                "timestamp": {
                    "type": "date"
                }
            }
        }
    },
    "priority": 500,
    "_meta": {
        "description": "my custom"
    }
}